{
  "name": "Neutrino",
  "version": "1.0.12",
  "description": "Neutrino AIO Bot",
  "main": "./webpack-pack/main.js",
  "author": {
    "name": "ThatOneAwkwardGuy",
    "email": "support@neutrino.app"
  },
  "repository": "https://github.com/ThatOneAwkwardGuy/Neutrino-Updates",
  "license": "SEE LICENSE IN LICENSE FILE",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "build": {
    "extends": null,
    "appId": "com.neutrino.bot",
    "directories": {},
    "afterSign": "scripts/notarize.js",
    "mac": {
      "category": "com.neutrino.bot",
      "icon": "./webpack-pack/icon.png",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "./entitlements.mac.plist",
      "entitlementsInherit": "./entitlements.mac.plist"
    },
    "win": {
      "icon": "./webpack-pack/icon.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "files": [
      "webpack-pack/"
    ],
    "extraResources": [
      {
        "from": "webpack-pack/captchaPreload.js",
        "to": "webpack-pack/captchaPreload.js"
      },
      {
        "from": "webpack-pack/activityPreload.js",
        "to": "webpack-pack/activityPreload.js"
      }
    ],
    "publish": {
      "provider": "github",
      "private": false
    }
  },
  "dependencies": {
    "@google-cloud/compute": "^0.11.0",
    "aws-sdk": "^2.473.0",
    "bootstrap": "^4.3.1",
    "cheerio": "^1.0.0-rc.2",
    "convert-csv-to-json": "0.0.14",
    "csvtojson": "^2.0.8",
    "discord-rpc": "^3.0.2",
    "do-wrapper": "^3.22.0",
    "dotenv": "^8.0.0",
    "electron-store": "^3.2.0",
    "electron-updater": "^4.0.6",
    "electron-window-manager": "^1.0.6",
    "firebase": "^6.1.0",
    "font-awesome": "^4.7.0",
    "json-2-csv": "^3.5.4",
    "lodash": "^4.17.11",
    "node-machine-id": "^1.1.12",
    "npm": "^6.9.0",
    "prop-types": "^15.7.2",
    "random-email": "^1.0.3",
    "random-name": "^0.1.2",
    "randomatic": "^3.1.1",
    "react": "^16.8.6",
    "react-beautiful-dnd": "^11.0.3",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-dom": "^16.8.6",
    "react-dropzone": "^10.1.5",
    "react-fontawesome": "^1.6.1",
    "react-particles-js": "^2.6.0",
    "react-redux": "^7.0.3",
    "react-router": "5.0.0",
    "react-router-dom": "^5.0.0",
    "react-spinners": "^0.5.4",
    "react-svg": "^10.0.6",
    "react-table": "^6.10.0",
    "react-toggle": "^4.0.2",
    "react-transition-group": "^4.1.0",
    "reactstrap": "^8.0.0",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-persist-electron-storage": "^2.1.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "uuid": "^3.3.2",
    "ws": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-helper-evaluate-path": "^0.5.0",
    "babel-loader": "^8.0.6",
    "babel-minify": "^0.5.0",
    "babel-plugin-emotion": "^10.0.9",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-minify": "^0.5.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "browser-sync": "^2.26.7",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron": "^5.0.2",
    "electron-builder": "^20.43.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-mocha": "^8.0.2",
    "electron-notarize": "^0.1.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^3.0.1",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "npm-run-all": "^4.1.5",
    "react-svg-loader": "^3.0.3",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "svg-react-loader": "^0.4.6",
    "terser-webpack-plugin": "^1.3.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.5.1",
    "webpack-livereload-plugin": "^2.2.0",
    "webpack-obfuscator": "^0.18.0"
  },
  "scripts": {
    "develop": "npm run private:compile -- --source-maps true && run-p -r private:watch private:serve",
    "dist": "cross-env electron-builder -mw --x64 -p always",
    "lint": "eslint --no-ignore scripts app test *.js",
    "pack": "run-s private:clean private:compile private:build:all",
    "pack:linux": "run-s private:clean private:compile private:build:linux",
    "pack:mac": "run-s private:clean private:compile private:build:mac",
    "pack:win": "run-s private:clean private:compile private:build:win",
    "postinstall": "electron-builder install-app-deps",
    "private:build:all": "electron-builder -mwl",
    "private:build:linux": "electron-builder --linux",
    "private:build:mac": "electron-builder --mac",
    "private:build:win": "electron-builder --win",
    "private:clean": "rimraf build",
    "private:compile": "babel app/ --copy-files --out-dir build",
    "private:serve": "babel-node scripts/serve.js",
    "private:watch": "npm run private:compile -- --source-maps true --watch --skip-initial-build",
    "test": "electron-mocha --renderer -R spec --require babel-core/register test/**/*.spec.js",
    "webpack:build:dev": "run-s webpack:prod:clean webpack:dev:pack webpack:dev:build",
    "webpack:build": "run-s webpack:prod:clean webpack:prod:pack webpack:prod:build",
    "webpack:build:mac": "run-s webpack:prod:clean webpack:prod:pack webpack:prod:build:mac",
    "webpack:build:win": "run-s webpack:prod:clean webpack:prod:pack webpack:prod:build:win",
    "webpack:dev": "cross-env NODE_ENV=development run-p webpack:dev:server webpack:dev:start",
    "webpack:dev:prod": "cross-env NODE_ENV=production run-p webpack:prod:server webpack:prod:start",
    "webpack:dev:pack": "webpack --mode development --config webpack.dev.config.js",
    "webpack:dev:server": "webpack --watch --config webpack.dev.config.js",
    "webpack:prod:server": "webpack --watch --config webpack.build.config.js",
    "webpack:dev:start": "cross-env BABEL_ENV=electron electron -r babel-register webpack-pack/main.js",
    "webpack:prod:start": "cross-env BABEL_ENV=electron electron -r babel-register webpack-pack/main.js",
    "webpack:dev:windows": "run-p webpack:dev:server webpack:dev:start",
    "webpack:dist": "run-s webpack:prod:clean webpack:prod:pack webpack:prod:dist",
    "webpack:prod": "run-s webpack:prod:clean webpack:prod:pack",
    "webpack:prod:build": "cross-env NODE_ENV=production electron-builder -mw --x64 --ia32",
    "webpack:dev:build": "cross-env NODE_ENV=development electron-builder -mw --x64 --ia32",
    "webpack:prod:build:mac": "cross-env NODE_ENV=production electron-builder -m --x64",
    "webpack:prod:build:win": "cross-env NODE_ENV=production electron-builder -w --x64",
    "webpack:prod:clean": "rimraf webpack-pack dist",
    "webpack:prod:dist": "cross-env NODE_ENV=production GH_TOKEN=22658cb28e268b0a1bbe49b10a86a1b1b9407490 electron-builder -mw --x64 --ia32 -p always",
    "webpack:prod:pack": "cross-env NODE_ENV=production webpack --mode production --config webpack.build.config.js"
  }
}
